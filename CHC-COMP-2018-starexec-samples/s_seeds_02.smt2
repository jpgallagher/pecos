(set-logic HORN)
(set-info :status sat)
(declare-fun itp (Int Int Int Int Int ) Bool)

(assert (forall ((x1 Int) (x2 Int) (x3 Int) (x4 Int) (x5 Int))
  (=> (and (= x1 2) (= x2 2) (= x3 2) (= x4 2) (= x5 2)) (itp x1 x2 x3 x4 x5))))
(assert (forall ((x1 Int)
         (x3 Int)
         (x5 Int)
         (x7 Int)
         (x9 Int)
         (x2 Int)
         (x4 Int)
         (x6 Int)
         (x8 Int)
         (x0 Int))
  (=> (and (itp x1 x3 x5 x7 x9)
           (= x2 (+ x1 1))
           (= x4 (+ x3 1))
           (= x6 (+ x5 1))
           (= x8 (+ x7 1))
           (= x0 (+ x9 1)))
      (itp x2 x4 x6 x8 x0))))
(assert (forall ((x1 Int) (x3 Int) (x5 Int) (x7 Int) (x9 Int))
  (let ((a!1 (and (itp x1 x3 x5 x7 x9)
                  (not (and (> x1 0) (> x3 0) (> x5 0) (> x7 0) (> x9 0))))))
    (=> a!1 false))))
(check-sat)

